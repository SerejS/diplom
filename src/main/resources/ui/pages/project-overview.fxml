<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="362.0" prefWidth="719.0" spacing="20.0" stylesheets="@../style/index.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.serejs.diplom.desktop.ui.controllers.ProjectOverviewController">
   <Label alignment="CENTER" prefHeight="30.0" prefWidth="230.0" text="Выберите проект">
      <font>
         <Font size="24.0" />
      </font>
   </Label>
    <ListView fx:id="projectList" cacheShape="false" centerShape="false" focusTraversable="false" prefHeight="396.0" prefWidth="679.0" scaleShape="false" style="-fx-border-color: #D7D2FB; -fx-border-width: 3px;" stylesheets="@../style/index.css" />
   <HBox alignment="CENTER" prefHeight="149.0" prefWidth="431.0" spacing="20.0">
      <Button onAction="#openLiteratures" mnemonicParsing="false" text="Типы литературы"/>
      <Button fx:id="createButton" onAction="#onCreateProject" mnemonicParsing="false" text="Создать проект"/>
      <Button fx:id="deleteButton" onAction="#deleteRow" disable="true" mnemonicParsing="false" text="Удалить проект"/>
   </HBox>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>

</VBox>
