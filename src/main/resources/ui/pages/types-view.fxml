<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<VBox alignment="CENTER" prefHeight="500.0" prefWidth="665.0" spacing="20.0" style="-fx-background-color: D7D2FB;"
      xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.serejs.diplom.desktop.ui.controllers.TypesController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <Label text="Просмотр типов литературы">
        <font>
            <Font size="20.0"/>
        </font>
    </Label>
    <TableView fx:id="table" prefHeight="397.0" prefWidth="625.0"/>
    <HBox alignment="CENTER" prefHeight="26.0" prefWidth="271.0" spacing="50.0">
        <Button fx:id="prevButton" mnemonicParsing="false" onAction="#goPrevPage" text="Список проектов"/>
        <Button fx:id="deleteButton" disable="true" mnemonicParsing="false" onAction="#deleteRow" prefHeight="25.0"
                prefWidth="133.0" text="Удалить" textAlignment="CENTER"/>
        <Button fx:id="addButton" mnemonicParsing="false" onAction="#openModal" prefHeight="25.0" prefWidth="133.0"
                text="Добавить"/>
    </HBox>
</VBox>
