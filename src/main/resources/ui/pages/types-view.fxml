<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="500.0" prefWidth="748.0" spacing="20.0" stylesheets="@../style/index.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.serejs.diplom.desktop.ui.controllers.pages.TypesController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <Label text="Просмотр типов литературы">
        <font>
            <Font size="26.0" />
        </font>
    </Label>
    <TableView fx:id="table" prefHeight="347.0" prefWidth="789.0" />
    <HBox alignment="CENTER" prefHeight="26.0" prefWidth="271.0" spacing="50.0">
        <Button fx:id="prevButton" mnemonicParsing="false" onAction="#goPrevPage" prefHeight="35.0" prefWidth="193.0" text="Список проектов" />
        <Button fx:id="deleteButton" disable="true" mnemonicParsing="false" onAction="#deleteRow" prefHeight="25.0" prefWidth="133.0" text="Удалить" textAlignment="CENTER" />
        <Button fx:id="addButton" mnemonicParsing="false" onAction="#openModal" prefHeight="25.0" prefWidth="133.0" text="Добавить" />
    </HBox>
</VBox>
