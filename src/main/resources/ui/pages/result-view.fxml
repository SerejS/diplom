<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="452.0" prefWidth="509.0" spacing="20.0" style="-fx-background-color: D7D2FB;"
      stylesheets="@../style/index.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.serejs.diplom.desktop.ui.controllers.pages.ResultController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <Label text="Результат анализа">
        <font>
            <Font name="System Bold" size="22.0"/>
        </font>
    </Label>
    <TextArea fx:id="area" editable="false" prefHeight="341.0" prefWidth="469.0">
        <font>
            <Font size="18.0"/>
        </font>
    </TextArea>
    <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="20.0">
        <children>
            <Button fx:id="finishButton" mnemonicParsing="false" onAction="#goProjectOverview" text="Сохранить"/>
            <Button onAction="#saveMdFile" mnemonicParsing="false" text="Экспорт в md"/>
        </children>
    </HBox>


</VBox>
